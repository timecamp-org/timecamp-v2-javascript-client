{
  "name": "timecamp-v2-javascript-client",
  "version": "0.0.2",
  "description": "TimeCamp API",
  "repository": {
    "type": "git",
    "url": "https://github.com/timecamp/timecamp-v2-javascript-client"
  },
  "main": "dist/timecamp-api.main.js",
  "scripts": {
    "prebuild": "npm run build:clean ###&& npm run test",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --display-modules --display-error-details --bail",
    "build:clean": "npm run test:clean && rimraf ./dist",
    "pretest": "npm run test:clean ###&& npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:watch": "cross-env CHROME_BIN=/usr/bin/chromium-browser npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-1": "^6.13.0",
    "babel-polyfill": "6.13.0",
    "babel-regenerator-runtime": "^6.5.0",
    "cross-env": "^2.0.1",
    "expect": "1.20.2",
    "expect-jsx": "2.6.0",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.6.5",
    "isparta": "4.0.0",
    "isparta-loader": "2.0.0",
    "json-loader": "0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.2.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "mocha": "3.0.2",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "class-autobind": "^0.1.4",
    "pouchdb": "^6.0.5",
    "pouchdb-browser": "^6.0.5",
    "string-format": "^0.5.0"
  }
}
